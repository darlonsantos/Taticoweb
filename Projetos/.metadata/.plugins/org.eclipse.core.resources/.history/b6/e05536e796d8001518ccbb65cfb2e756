package br.edu.devmedia.entity;

import java.util.Map;

public class Pessoa {

	private String nome;

	private Pessoa[][] filhos;
	
	private Map<String, String> nomes;

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public Pessoa[][] getFilhos() {
		return filhos;
	}

	public void setFilhos(Pessoa[][] filhos) {
		this.filhos = filhos;
	}

	public Map<String, String> getNomes() {
		return nomes;
	}

	public void setNomes(Map<String, String> nomes) {
		this.nomes = nomes;
	}

}
