package br.edu.devmedia.entity;

import java.util.List;

public class Pessoa {

	private String nome;

	private Pessoa[] filhos;

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public Pessoa[] getFilhos() {
		return filhos;
	}

	public void setFilhos(Pessoa[] filhos) {
		this.filhos = filhos;
	}

}
